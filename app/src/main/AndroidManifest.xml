<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Team4.TTA">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/playroom"
        android:label="@string/app_name"
        android:roundIcon="@drawable/playroom"
        android:supportsRtl="false"
        android:theme="@style/Theme.MathForKids">
        <activity android:name=".LearnOrPlay123"/>
        <activity android:name=".LearnOrPlayABC"/>
        <activity android:name=".learn_123"/>
        <activity android:name=".learn_alphabet"/>
        <activity android:name=".Start" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".EditProfile" />
        <activity android:name=".Profile" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login" />
        <activity
            android:name=".GameActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/AD_MOB_APP_ID" />
    </application>

</manifest>